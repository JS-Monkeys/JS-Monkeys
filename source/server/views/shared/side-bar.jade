.collapse.navbar-collapse.navbar-ex1-collapse
  ul.nav.navbar-nav.side-nav

    li
      a(href='javascript:;', data-toggle='collapse', data-target='#contests')
        i.fa.fa-fw.fa-trophy
        |  Contests
        i.fa.fa-fw.fa-caret-down

      ul#contests.collapse
        each contest in ['contest 1', 'contest 2']
          li
            a(href='contests/' + contest) #{contest}
    li
      a(href='javascript:;', data-toggle='collapse', data-target='#courses')
        i.fa.fa-fw.fa-book
        |  Courses
        i.fa.fa-fw.fa-caret-down

      ul#courses.collapse
        each course in ['course 1', 'course 2']
          li
            a(href='courses/' + course) #{course}

    li
      a(href='/rankings')
        i.fa.fa-fw.fa-star
        |  Rankings

    li
      a(href='/about')
        i.fa.fa-fw.fa-info-circle
        |  About
